  "scripts": {
    "build": "parcel build forexvouchers/static/js/forexvouchers.js forexvouchers/static/styles/base.scss -d forexvouchers/static/dist",
    "build:styles": "parcel build forexvouchers/static/styles/base.scss --no-minify -d forexvouchers/static/dist/styles",
    "build:static": "npm run build; python manage.py collectstatic --noinput",
    "watch": "parcel forexvouchers/static/js/* forexvouchers/static/styles/* -d forexvouchers/static/dist",
    "watch:styles": "nodemon --watch forexvouchers/static/styles/**/*.scss -e scss --exec 'npm run build:styles'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
